{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "types": ["vite/client"]
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
                ],
    "exclude": ["node_modules"]
  }